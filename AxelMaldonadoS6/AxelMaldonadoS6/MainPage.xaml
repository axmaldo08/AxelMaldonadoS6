<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AxelMaldonadoS6.MainPage">

    <StackLayout BackgroundColor="#EAE33E">

        <Grid>
            <Grid.RowDefinitions>

                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>

            <Image Source="centro.png" Grid.Row="1" Grid.Column="0" HorizontalOptions="Center"/>
            <Button  x:Name="btnCentro" Text="Centro de Vacunación" Clicked="btnCentro_Clicked" Grid.Row="2" Grid.Column="0" FontSize="30" />

        </Grid>


    </StackLayout>

</ContentPage>
